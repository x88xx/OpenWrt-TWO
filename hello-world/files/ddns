
config ddns 'global'
	option ddns_dateformat '%F %R'
	option ddns_loglines '250'
	option upd_privateip '0'

config service 'aliyun'
	option service_name 'aliyun.com'
	option enabled '0'
	option username ''
	option password ''
	option ip_source 'web'
	option ip_url 'http://4.ipw.cn'
	option interface 'lan'
	option use_syslog '0'
	option lookup_host ''
	option domain ''

config service 'aliyun_ipv6'
	option use_ipv6 '1'
	option service_name 'aliyun.com'
	option enabled '0'
	option username ''
	option password ''
	option ip_source 'web'
	option ip_url 'http://6.ipw.cn'
	option interface 'lan'
	option use_syslog '0'
	option lookup_host ''
	option domain ''

